<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="286.0" prefWidth="423.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="meviusmoebelhouse.gui.controllers.LoginController">

    <children>
        <Label layoutX="93.0" layoutY="71.0" text="Username" />
        <Label layoutX="95.0" layoutY="110.0" text="Password" />
        <Button fx:id="loginButton" layoutX="267.0" layoutY="149.0" mnemonicParsing="false" onAction="#loginOCE" text="Login" />
        <TextField fx:id="usernameField" layoutX="164.0" layoutY="67.0" promptText="Enter username" />
        <TextField fx:id="passwordField" layoutX="164.0" layoutY="106.0" promptText="Enter password" />
        <Button fx:id="registerButton" layoutX="95.0" layoutY="149.0" mnemonicParsing="false" onAction="#registerOCE" text="Register" />
        <Label fx:id="errorMessageLabel" layoutX="93.0" layoutY="222.0" />
        <Button fx:id="backToHomeButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToHomeOCE" text="Back to home" />
    </children>

</AnchorPane>
